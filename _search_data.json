---
layout: null
---
{
  {% for product in site.products  %}

    "{{ product.url | slugify }}": {
      "name": "{{ product.name | xml_escape }}",
      "content"	 : "{{product.content | strip_html | strip_newlines | remove:  "	" | escape | remove: "\"}}",
      "url": " {{ product.url | xml_escape }}",
      "code": " {{ product.code | xml_escape }}",
      "family": "{{ product.family | xml_escape }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
}
